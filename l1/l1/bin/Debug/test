
define int array Swap(int array a, int n, int j, int i)
   assert i < n and j < n;
   int temp := a[i];
   a[i] := a[j];
   a[j] := temp;
   return a;
end

*** Hidden input
define int array BubbleSort(int array a, int n)
    int j := 0 to n - 1 step 1 do
        int i := 0 to n - 1 - j step 1 do
            if a[i] > a[i+1] then
                a := Swap(a, n, i, i+1);
            end;
        end;
    end;
    return a;
end 

define Main()
 int n := 10;
 PrintString("BEFORE:");
 int array a := new int[n];
 int i := 0 to n do
    a[i] := n - i;
    PrintInt(a[i]);
 end;
 a := BubbleSort(a, n);
 PrintString("AFTER:");
 i := 0 to n do
    PrintInt(a[i]);
 end;
 char array array s := new char array array[10];
 s[0] := "one";
 s[1]:= "two";
 char array h := "hello";
 PrintString(h);
end

